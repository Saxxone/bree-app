{
  "expo": {
    "name": "Bree",
    "slug": "bree-app",
    "description": "Bree Social is social media that's healthy and non-toxic. Social media that is the best of us for us. Bree is freedom to express yourself in the most authentic way. Be Free",
    "version": "0.0.1",
    "orientation": "portrait",
    "icon": "./app_directories/assets/images/bree.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "platforms": [
      "android",
      "ios"
    ],
    "notification": {
      "icon": "./app_directories/assets/images/bree.png",
      "color": "#6366f1",
      "iosDisplayInForeground": true,
      "androidMode": "default"
    },
    "splash": {
      "image": "./app_directories/assets/images/bree.png",
      "resizeMode": "contain",
      "backgroundColor": "#1f2937"
    },
    "updates": {
      "fallbackToCacheTimeout": 0,
      "checkAutomatically": "ON_LOAD"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "social.bree.app"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./app_directories/assets/images/bree.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.anonymous.breeapp"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./app_directories/assets/images/bree.png"
    },
    "plugins": [
      [
        "expo-secure-store",
        {
          "configureAndroidBackup": true,
          "faceIDPermission": "Allow Bree to access your Face ID biometric data."
        }
      ],
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./app_directories/assets/images/bree.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      [
        "expo-video",
        {
          "supportsBackgroundPlayback": true,
          "supportsPictureInPicture": true
        }
      ],
      "expo-localization",
      "expo-secure-store"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "b07a926a-e829-4cc0-980d-a9b765cfca4c"
      }
    },
    "owner": "saxxone"
  }
}
