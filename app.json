{
  "expo": {
    "name": "Bree",
    "slug": "bree",
    "description": "Bree Social is social media that's healthy and non-toxic. Social media that is the best of us for us. Bree is freedom to express yourself in the most authentic way. Be Free",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./app/assets/images/bree.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "social.bree.app"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./app/assets/images/bree.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.anonymous.breeapp"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./app/assets/images/bree.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./app/assets/images/bree.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        },
      ],
      [
        "expo-video",
        {
          "supportsBackgroundPlayback": true,
          "supportsPictureInPicture": true
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "b07a926a-e829-4cc0-980d-a9b765cfca4c"
      }
    },
    "owner": "saxxone"
  }
}
